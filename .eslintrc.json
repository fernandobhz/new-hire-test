{
    "env": {
      "es2020": true,
      "node": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": 0,
      "arrow-parens": "off",
      "no-extra-parens": "error"
    },
    "ignorePatterns": "dist"
  }
  